<div style="width: 1230px;margin: 2rem 0px;">
  <div
    style="
      display: flex;
      justify-content: space-between;
      padding: 20px;
      border: solid;
      border-width: 1px;
      align-items: center;
      position: relative;
    "
  >
    <div>
      <img src="/images/ic-pertaarungas.png" style="width: 100px" />
    </div>
    <div style="display: flex; flex-direction: column; align-items: center">
      <span style="padding: 3px 0px; font-weight: 700; font-size: 20px"
        >POWER GENERATION SYSTEM UNIT - 90</span
      >
      <span style="padding: 3px 0px; font-weight: 700; font-size: 20px"
        >PG-9001</span
      >
      <span style="padding: 8px 0px 0px 0px; font-weight: 600; font-size: 17px">
        Date : <%= data.queryDate %>
      </span>
    </div>
    <div>
      <span>PAG-408</span>
    </div>
    <div style="position: absolute; right: 9px; bottom: 0px">
      <h5>SHEET : 4 OF 5</h5>
    </div>
  </div>
  <div>
    <table class="styleBorder" style="width: 1230px">
      <thead>
        <tr>
          <th rowspan="3" style="padding: 50px 10px">Hours</th>

          <th colspan="4" style="padding: 10px 0px">
            FUEL GAS PRESS
          </th>
          <th colspan="2">COMP. DISC. AIR PRESS</th>
          <th colspan="5">DIFF. PRESS</th>
          <th rowspan="2" class="verticalTable">FUEL DIESEL LEVEL</th>
          <th rowspan="2" class="verticalTable">LUBE OIL LEVEL</th>
          <th colspan="2">LEL</th>
          <th colspan="2">COOLING WATER</th>
          <th rowspan="2" class="verticalTable">AMBIENT TEMP.</th>
          <th rowspan="2" >GAS </br> CONSUMPTION</th>
          <th rowspan="2" >REMARKS</th>
        </tr>
        <tr>
          <th class="verticalTable">HP GAS BEFORE </br> KOD</th>
          <th class="verticalTable">BEFORE STOP </br> VALVE</th>
          <th class="verticalTable">AFTER GAS STOP </br> VALVE</th>
          <th class="verticalTable">AFTER GAS </br> CONTROL VALVE</th>
          <!--  -->
          <th class="verticalTable">AFTER POROUS </br> FILTER</th>
          <th class="verticalTable">'( For 96 CD )</th>

          <!--  -->
          <th class="verticalTable">INLET HOUSE </br> FILTER</th>
          <th class="verticalTable">LUBE OIL FILTER</th>
          <th class="verticalTable">CONTROL OIL </br>  FILTER</th>
          <th class="verticalTable">HYD. OIL FILTER</th>
          <th class="verticalTable">AIR COOLER </br> FILTER</th>
          <!--  -->
          <th class="verticalTable">DETECTOR NO.1</th>
          <th class="verticalTable">DETECTOR NO.2</th>
          <!--  -->
          <th class="verticalTable">FLOW</th>
          <th class="verticalTable">TEMPERATURE</th>
          <!--  -->
        </tr>
        <tr>
          <th colspan="11" class="paddingTable">KG/CM²</th>
          <th colspan="4"class="paddingTable"></th>
          <th class="paddingTable">M³/H</th>
          <th colspan="2"class="paddingTable">° C</th>
          <th class="paddingTable">M³</th>
          <th class="paddingTable"></th>
        </tr>
      </thead>
      <tbody style="text-align: center">
        <!--  -->
        <% for(let i = 0; i < data.temp.length; i++) { %>
          <tr>
          <td><%= data.temp[i].clock %></td>
          <td><%= data.temp[i].value.get_tbl_fuelgas_press[0]?.value_hpgas_before %></td>
          <td><%= data.temp[i].value.get_tbl_fuelgas_press[0]?.value_beforestop_value %></td>
          <td><%= data.temp[i].value.get_tbl_fuelgas_press[0]?.value_aftergas_stopvalue %></td>
          <td><%= data.temp[i].value.get_tbl_fuelgas_press[0]?.value_aftergas_controlvalue %></td>
          <td><%= data.temp[i].value.get_tbl_compdisch_airpress[0]?.value_afterporous_filter %></td>
          <td><%= data.temp[i].value.get_tbl_compdisch_airpress[0]?.value_for96cd %></td>
          <td><%= data.temp[i].value.get_tbl_diffpress[0]?.value_inlethouse_filter %></td>
          <td><%= data.temp[i].value.get_tbl_diffpress[0]?.value_lubeoil_filter %></td>
          <td><%= data.temp[i].value.get_tbl_diffpress[0]?.value_controloil_filter %></td>
          <td><%= data.temp[i].value.get_tbl_diffpress[0]?.value_hydoil_filter %></td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>

          <td></td>
          <td><%= data.temp[i].value.get_tbl_cooling_water[0]?.value_temperature %></td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
        <%}%>
      </tbody>
    </table>
  </div>
  <div style="border: solid; border-width: 1px; display: flex">
    <div class="signature" style="position: relative">
      <span style="position: absolute; top: 10px; left: 10px"
        >00:00 ~ 08:00</span
      >
      <div
        style="
          display: flex;
          justify-content: space-between;
          padding: 0px 5px;
          position: absolute;
          bottom: 0px;
        "
      >
        <h4 style="padding-right: 10px">Opr.: ____________</h4>
        <h4>Supv./Panelman : ____________</h4>
      </div>
    </div>
    <div class="signature" style="position: relative">
      <span style="position: absolute; top: 10px; left: 10px"
        >08:00 ~ 16:00</span
      >
      <div
        style="
          display: flex;
          justify-content: space-between;
          padding: 0px 5px;
          position: absolute;
          bottom: 0px;
        "
      >
        <h4 style="padding-right: 10px">Opr.: ____________</h4>
        <h4>Supv./Panelman : ____________</h4>
      </div>
    </div>
    <div class="signature" style="position: relative">
      <span style="position: absolute; top: 10px; left: 10px"
        >16:00 ~ 00:00</span
      >
      <div
        style="
          display: flex;
          justify-content: space-between;
          padding: 0px 5px;
          position: absolute;
          bottom: 0px;
        "
      >
        <h4 style="padding-right: 10px">Opr.: ____________</h4>
        <h4>Supv./Panelman : ____________</h4>
      </div>
    </div>
  </div>
  
</div>
